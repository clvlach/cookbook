{
  "name": "cookbook",
  "description": "An online cookbook",
  "version": "0.0.2",
  "homepage": "https://github.com/clvlach/cookbook",
  "repository": "https://github.com/clvlach/cookbook",
  "private": true,
  "license": "Apache 2",
  "scripts": {
    "clean": "rimraf dist",
    "build:client": "rollup --config",
    "build:server": "tsc",
    "build": "run-p build:*",
    "watch:client": "rollup --config --watch",
    "watch:server": "tsc --watch",
    "start:dev": "node-dev  --require source-map-support/register --require dotenv/config dist/app.js",
    "dev": "npm-run-all clean build -p watch:* start:dev",
    "prestart": "npm run build",
    "start": "node --require dotenv/config dist/app.js",
    "validate": "svelte-check",
    "test": "exit 1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/express": "^4.17.7",
    "@types/mysql2": "github:types/mysql2",
    "node-dev": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "schema-dts": "^0.6.0",
    "svelte": "^3.0.0",
    "svelte-check": "^0.1.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "sirv": "^1.0.1",
    "mysql2": "^2.1.0"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not IE 11",
      "not IE_Mob 11"
    ],
    "ssr": [
      "node 12"
    ]
  }
}
